<Window x:Class="NumberGame.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NumberGame"
        mc:Ignorable="d"
        Title="MainWindow" Height="630" Width="600">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="50"/>
            <RowDefinition Height="100"/>
            <RowDefinition Height="100"/>
            <RowDefinition Height="100"/>
            <RowDefinition Height="100"/>
            <RowDefinition Height="100"/>
            <RowDefinition Height="50"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="120"/>
            <ColumnDefinition Width="120"/>
            <ColumnDefinition Width="120"/>
            <ColumnDefinition Width="120"/>
            <ColumnDefinition Width="120"/>
        </Grid.ColumnDefinitions>

        <Button x:Name="bt1" Grid.Column="0" Grid.Row="1" Content="1" FontSize="50" Click="Button_Click"/>
        <Button x:Name="bt2" Grid.Column="1" Grid.Row="1" Content="2" FontSize="50" Click="Button_Click"/>
        <Button x:Name="bt3" Grid.Column="2" Grid.Row="1" Content="3" FontSize="50" Click="Button_Click"/>
        <Button x:Name="bt4" Grid.Column="3" Grid.Row="1" Content="4" FontSize="50" Click="Button_Click"/>
        <Button x:Name="bt5" Grid.Column="4" Grid.Row="1" Content="5" FontSize="50" Click="Button_Click"/>
        <Button x:Name="bt6" Grid.Column="0" Grid.Row="2" Content="6" FontSize="50"/>
        <Button x:Name="bt7" Grid.Column="1" Grid.Row="2" Content="7" FontSize="50"/>
        <Button x:Name="bt8" Grid.Column="2" Grid.Row="2" Content="8" FontSize="50"/>
        <Button x:Name="bt9" Grid.Column="3" Grid.Row="2" Content="9" FontSize="50"/>
        <Button x:Name="bt10" Grid.Column="4" Grid.Row="2" Content="10" FontSize="50"/>
        <Button x:Name="bt11" Grid.Column="0" Grid.Row="3" Content="11" FontSize="50"/>
        <Button x:Name="bt12" Grid.Column="1" Grid.Row="3" Content="12" FontSize="50"/>
        <Button x:Name="bt13" Grid.Column="2" Grid.Row="3" Content="13" FontSize="50"/>
        <Button x:Name="bt14" Grid.Column="3" Grid.Row="3" Content="14" FontSize="50"/>
        <Button x:Name="bt15" Grid.Column="4" Grid.Row="3" Content="15" FontSize="50"/>
        <Button x:Name="bt16" Grid.Column="0" Grid.Row="4" Content="16" FontSize="50"/>
        <Button x:Name="bt17" Grid.Column="1" Grid.Row="4" Content="17" FontSize="50"/>
        <Button x:Name="bt18" Grid.Column="2" Grid.Row="4" Content="18" FontSize="50"/>
        <Button x:Name="bt19" Grid.Column="3" Grid.Row="4" Content="19" FontSize="50"/>
        <Button x:Name="bt20" Grid.Column="4" Grid.Row="4" Content="20" FontSize="50"/>
        <Button x:Name="bt21" Grid.Column="0" Grid.Row="5" Content="21" FontSize="50"/>
        <Button x:Name="bt22" Grid.Column="1" Grid.Row="5" Content="22" FontSize="50"/>
        <Button x:Name="bt23" Grid.Column="2" Grid.Row="5" Content="23" FontSize="50"/>
        <Button x:Name="bt24" Grid.Column="3" Grid.Row="5" Content="24" FontSize="50"/>
        <Button x:Name="bt25" Grid.Column="4" Grid.Row="5" Content="25" FontSize="50"/>
        
    </Grid>
</Window>
